<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
   <item android:state_pressed="true">
       <shape >
          <!-- 描边 -->
           <stroke 
               android:dashGap="0dp"
               android:width="4dp"
               android:color="#60aaf2"/>
              <!-- 填充颜色-->
           <solid android:color="#ffffff"/>
             <!-- 圆角半径-->
           <corners android:radius="10dp"/>
       </shape>
   </item> 
  

</selector>